<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.towson.cosc431.christian.hangman">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name=".MyIntentService"
            android:enabled="true"
            android:exported="true"
            ></service>

        <activity android:name=".TwoPlayer"></activity>
        <activity android:name=".TwoGame" />
        <activity android:name=".CustomGame" />
        <activity android:name=".CustomInput" />
        <activity android:name=".Score" />
        <activity android:name=".Singlegame" />
        <activity android:name=".Singleplayer" />
        <activity android:name=".Background" />
        <activity android:name=".Settings" />
        <activity android:name=".Choice" />
        <activity android:name=".Login" />
        <activity android:name=".Registration" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>